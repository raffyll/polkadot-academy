programa {
  funcao inicio() 
  {
    real a, b, c, media

    escreva("Digite o primeiro número: ")
    leia(a)

    escreva("Digite o segundo número: ")
    leia(b)

    escreva("Digite o terceiro número: ")
    leia(c)

    media = (a + b + c) / 3

    escreva("A média dos 3 números é: ", media)
  }
}
